{
    "appenders": [
        {
            "type": "console"
        },
        {
            "type": "file",
            "filename": "./logs/Test.log",
            "pattern": "-yyyy-MM-dd",
            "alwaysIncludePattern": false
        },
        {
            "category": "testResults",
            "type": "file",
            "filename": "./logs/testResults.log",
            "pattern": "-yyyy-MM-dd",
            "alwaysIncludePattern": false
        }
    ],

    "replaceConsole": true,
    "levels": {
        "[all]": "Info",
        "FFMpegTask": "error"
    }
}
